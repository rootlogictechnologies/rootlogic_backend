{
  "kind": "collectionType",
  "collectionName": "applicants",
  "info": {
    "singularName": "applicant",
    "pluralName": "applicants",
    "displayName": "applicant"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email",
      "unique": true,
      "required": true
    },
    "phoneNo": {
      "type": "string",
      "unique": true
    },
    "linkedInURL": {
      "type": "string"
    },
    "portfolioURL": {
      "type": "string"
    },
    "gitHubURL": {
      "type": "string"
    },
    "resume": {
      "allowedTypes": ["files"],
      "type": "media",
      "multiple": false
    },
    "jobOpenings": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::job-opening.job-opening",
      "inversedBy": "applicants"
    }
  }
}
